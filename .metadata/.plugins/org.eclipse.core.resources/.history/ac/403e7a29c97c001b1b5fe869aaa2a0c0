package cn.zhen77.sort;

public class BubboSort {
	public static void main(String[] args) {
		int[] arr ={6,3,8,2,1,5,7};
		int temp = 0 ;
		
		for(int i=0;i<arr.length;i++) {
			if(arr[i]>arr[i+1]) {
				temp=arr[i];
				arr[i]=arr[i+1];
				arr[i+1]=temp;
			}
		}
		SelectionSort.print(arr);
	}

	
}

